{
  "name": "vprof-frontend",
  "author": "nvdv",
  "main": "vprof/frontend/main.js",
  "dependencies": {
    "browserify": "13",
    "browserify-css": "latest",
    "d3": "4.9.1",
    "eslint": "latest",
    "highlight.js": "latest",
    "jest": "18.0.0",
    "uglify-es": "3.0.24",
    "watchify": "3.7.0"
  },
  "scripts": {
    "lint": "eslint vprof/frontend",
    "build": "browserify -g browserify-css vprof/frontend/main.js | uglifyjs -o vprof/frontend/vprof_min.js",
    "watch": "watchify vprof/frontend/main.js -o vprof/frontend/vprof_min.js -v",
    "test": "jest"
  },
  "browserify": {
    "transform": [
      "browserify-css"
    ]
  },
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "http://github.com/nvdv/vprof.git"
  }
}
